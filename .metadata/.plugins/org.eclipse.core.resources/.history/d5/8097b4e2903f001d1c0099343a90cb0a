package sprint0_product;

import java.awt.*;
import javax.swing.*;
import java.awt.*;


@SuppressWarnings("serial")
public class SOS_GAME_GUI extends JFrame {
	public SOS_GAME_GUI() {
		setSize(50, 50);
		setVisible(true);
	}
	public void paint( Graphics g) {
		int x;
		int y;
		for(x = 30; x <= 300; x += 30);
		for(y = 30; y <= 300; y += 30);
		g.drawRect(x, y, 30, 30);
	}
	public static void main(String args[]) {
		SOS_GAME_GUI application = new SOS_GAME_GUI();
		application.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
	}
	
	
	
	

}
