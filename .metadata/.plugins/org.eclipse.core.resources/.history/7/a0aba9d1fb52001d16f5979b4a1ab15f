package sprint2_product;

public class MyPair {
	private final Integer sym;
    private final String clr;

    public MyPair(Integer aSym, String aClr)
    {
        sym   = aSym;
        clr = aClr;
    }

    public Integer getSym()   { return sym; }
    public String getClr() { return clr; }


}
