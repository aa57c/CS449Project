package sprint2_product;

public class MyPair {
	private Integer sym;
    private String clr;

    public MyPair()
    {
        sym = 0;
        clr = "None";
    }
    public void setMyPair(Integer aSym, String aClr)
    {
        this.sym = aSym;
        this.clr = aClr;
    }
    public Integer getSym()   { return sym; }
    public String getClr() { return clr; }


}
